<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
  xmlns:form="sap.ui.layout.form"
  xmlns:layout="sap.ui.layout">
  
  <Dialog
    id="addValueDialog"
    title="Agregar Value"
    contentWidth="600px">
    
    <content>
      <form:SimpleForm id="_IDGenSimpleForm5" editable="true" layout="ResponsiveGridLayout">
                <Label id="_IDGenLabel36" text="LABELID" required="true"/>
        <Text id="_IDGenText73" text="{values>/selectedValue/LABELID}" />

        <Label id="_IDGenLabel37" text="VALUEID" required="true"/>
        <Input value="{newValueModel>/VALUEID}"  id="InputValueId"/>

        <Label id="_IDGenLabel38" text="VALUE" required="true"/>
        <Input id="_IDGenInput16" value="{newValueModel>/VALUE}" />

        <Label id="_IDGenLabel39" text="VALUEPAID" />
        <Input id="_IDGenInput17" value="{newValueModel>/VALUEPAID}" />

        <Label id="_IDGenLabel40" text="ALIAS" />
        <Input id="_IDGenInput18" value="{newValueModel>/ALIAS}" />

        <Label id="_IDGenLabel41" text="IMAGE" />
        <Input id="_IDGenInput19" value="{newValueModel>/IMAGE}" />

        <Label id="_IDGenLabel42" text="DESCRIPTION" />
        <TextArea id="_IDGenTextArea2" value="{newValueModel>/DESCRIPTION}" rows="3"/>
      </form:SimpleForm>
    </content>
    
    <beginButton>
      <Button id="_IDGenButton23" text="Guardar" type="Emphasized" press="onSaveValues"/>
    </beginButton>
    
    <endButton>
      <Button id="_IDGenButton24" text="Cancelar" press="onCancelValues"/>
    </endButton>
  </Dialog>
</core:FragmentDefinition>