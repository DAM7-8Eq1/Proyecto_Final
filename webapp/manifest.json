{
    "_version": "1.65.0",
    "sap.app": {
        "id": "com.inv.sapfiroriwebinversion",
        "type": "application",
        "i18n": {
            "bundleName": "com.inv.sapfiroriwebinversion.i18n.i18n",
            "fallbackLocale": "es_MX",
            "supportedLocales": [
            "es_MX"
            ]
        },  
        "applicationVersion": {
        "version": "1.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.16.4",
            "toolsId": "cc52223c-0384-4871-a654-19bb1ec85476"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.132.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.tnt": {},
                "sap.f": {},
                "sap.ui.table": {},
                "sap.ui.layout": {},
                "sap.viz": {},
                "sap.ui.comp": {},
                "sap.suite.ui.microchart": {}
            }
       },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                "bundleName": "com.inv.sapfiroriwebinversion.i18n.i18n",
                "fallbackLocale": "es_MX",
                "supportedLocales": [
                    "es_MX"
                ]
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "controlId": "mainContents",
                "viewType": "XML",
                "path": "com.inv.sapfiroriwebinversion.view",
                "async": true,
                "viewPath": "com.inv.sapfiroriwebinversion.view"
            },
            "routes": [
                {
                    "name": "main",
                    "pattern": "",
                    "target": [
                        "TargetMain"
                    ]
                },
                {
                    "name":"RouteSalesForecast",
                    "pattern":"salesforecast",
                    "target": ["TargetSalesForecast"]
                },
                {
                    "name":"RouteLogin",
                    "pattern":"login",
                    "target": ["TargetLogin"]
                },
                {
                    "name":"RouteUserList",
                    "pattern":"user",
                    "target": ["TargetUserList"]
                },
                {
                    "name":"RouteRolesMaster",
                    "pattern":"roles",
                    "target": ["TargetRolesMaster"]
                },
                {
                   "pattern": "investments",
                    "name": "RouteInvestments",
                    "target": ["TargetInvestments"]
                }
            ],
            "targets": {
                "TargetMain": {
                    "id": "Main",
                    "name": "Main"
                },
                "TargetSalesForecast":{
                    "viewId": "SalesForecast",
                    "viewName": "SalesForecast",
                    "viewType": "XML",
                    "transition":"slide",
                    "clearControlAggregation": false,
                    "viewlevel": 2,
                    "path": "com.inv.sapfiroriwebinversion.view.salesforecast",
                    "viewPath": "com.inv.sapfiroriwebinversion.view.salesforecast"
                },
                "TargetLogin":{
                    "viewId": "login",
                    "viewName": "Login",
                    "viewType": "XML",
                    "transition":"slide",
                    "clearControlAggregation": false,
                    "viewlevel": 2,
                    "path": "com.inv.sapfiroriwebinversion.view.security",
                    "viewPath": "com.inv.sapfiroriwebinversion.view.security"
                },
                "TargetUserList":{
                    "viewId": "UsersList",
                    "viewName": "UsersList",
                    "viewType": "XML",
                    "transition":"slide",
                    "clearControlAggregation": false,
                    "viewlevel": 2,
                    "path": "com.inv.sapfiroriwebinversion.view.security.pages",
                    "viewPath": "com.inv.sapfiroriwebinversion.view.security.pages"
                },
                "TargetRolesMaster":{
                    "viewId": "RolesMaster",
                    "viewName": "RolesMaster",
                    "viewType": "XML",
                    "transition":"slide",
                    "clearControlAggregation": false,
                    "viewlevel": 2,
                    "path": "com.inv.sapfiroriwebinversion.view.security.pages",
                    "viewPath": "com.inv.sapfiroriwebinversion.view.security.pages"
                },
                "TargetInvestments":{
                    "viewId": "Investments",
                    "viewName": "Investments",
                    "viewType": "XML",
                    "transition":"slide",
                    "clearControlAggregation": false,
                    "viewlevel": 2,
                    "path": "com.inv.sapfiroriwebinversion.view.investments.pages",
                    "viewPath": "com.inv.sapfiroriwebinversion.view.investments.pages"
                }
            }
        },
        "rootView": {
            "viewName": "com.inv.sapfiroriwebinversion.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}